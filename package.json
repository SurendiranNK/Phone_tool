{
    "name": "mTools",
    "version": "1.0.0",
    "description": "Mobile services",
    "main": "main.js",
    "scripts": {
        "rebuild": "electron-rebuild -f -w sqlite3",
        "start": "tsc && concurrently \"npm run tsc:w\" \"electron .\" ",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "package-linux": "electron-packager .  mTools --overwrite --asar=true --platform=linux --arch=x64 --icon=app/assets/loading-logo-img.png --prune=true --out=release-builds",
        "create-installer-win": "node installers/windows/createinstaller.js",
        "create-debian-installer": "electron-installer-debian --src release-builds/mTools-linux-x64/ --arch amd64 --config debian.json"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jiturddigital2016/mAutomation-B.git"
    },
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "Angular"
    ],
    "author": "Bhaskar Newase <newasebhaskar9@gmail.com>",
    "license": "",
    "bugs": {
        "url": "https://github.com/jiturddigital2016/mAutomation-B/issues"
    },
    "homepage": "https://github.com/jiturddigital2016/mAutomation-B/#readme",
    "dependencies": {
        "@angular/animations": "^4.2.6",
        "@angular/cdk": "^2.0.0-beta.8",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/material": "^2.0.0-beta.8",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "@angular/upgrade": "2.2.0",
        "@types/hammerjs": "^2.0.34",
        "adb": "^0.2.0",
        "adbkit": "^2.11.0",
        "bcrypt": "^1.0.2",
        "bluebird": "^3.5.0",
        "body-parser": "^1.13.3",
        "core-js": "^2.4.1",
        "electron-log": "^2.2.14",
        "electron-rebuild": "^1.7.3",
        "express": "^4.13.3",
        "fs": "0.0.1-security",
        "hammerjs": "^2.0.8",
        "mysql": "^2.13.0",
        "node-bluetooth": "^1.1.3",
